@model IEnumerable<RentalRecordViewModel>


<table>
    <thead>
    <tr>
        <th>Rental ID</th>
        <th>Rental Date</th>
        <th>Rental Due Date</th>
        <th>Rental Return Date</th>
        <th>Movie ID</th>
        <th>Movie Name</th>
        <th>Customer ID</th>
        <th>Customer Name</th>
        <th>Customer Phone Number</th>
    </tr>
    </thead>
<tr>
@foreach(var item in Model)
{
    <td>@item.RentalRecordID</td>
}
</tr>
<tr>
@foreach(var item in Model)
{
    <td>@item.RentalDate</td>
}
</tr>
<tr>
@foreach(var item in Model)
{   
    <td>@item.DueDate</td>
}
</tr>
<tr>
@foreach(var item in Model)
{
    <td>@item.ReturnDate</td>
}
</tr>
<tr>
@foreach(var item in Model)
{
    <td>@item.MoviesID</td>   
}
</tr>
<tr>
@foreach(var item in Model)
{
    <td>@item.MovieName</td>
}
</tr>
<tr>
@foreach(var item in Model)
{
    <td>@item.CustomerID</td>
}
</tr>
<tr>
@foreach(var item in Model)
{
    <td>@item.CustomerName</td>
}
</tr>
<tr>
@foreach(var item in Model)
{
    <td>@item.CustomerPhoneNumber</td>
}
</tr>
</table>